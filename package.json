{
  "name": "@kayahr/eslint-config",
  "version": "2.1.0",
  "description": "kayahr eslint config for TypeScript projects",
  "main": "eslintrc.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayahr/eslint-config"
  },
  "funding": "https://github.com/kayahr/eslint-config?sponsor=1",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "tsc",
    "prepare": "npm run compile",
    "lint": "eslint --max-warnings 0 --ext .ts src",
    "cspell": "cspell --no-progress",
    "test": "npm run cspell && npm run lint"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "cspell": "5.19.3",
    "eslint": "8.12.0",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">= 5.10.0",
    "@typescript-eslint/parser": ">= 5.10.0",
    "eslint": ">= 8.7.0",
    "eslint-plugin-deprecation": ">= 1.3.2",
    "eslint-plugin-simple-import-sort": ">= 7.0.0"
  }
}
