{
  "name": "@kayahr/eslint-config",
  "version": "1.6.1",
  "description": "kayahr eslint config for TypeScript projects",
  "main": "eslintrc.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayahr/eslint-config"
  },
  "funding": "https://github.com/kayahr/eslint-config?sponsor=1",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "tsc",
    "prepare": "npm run compile",
    "lint": "eslint --max-warnings 0 --ext .ts src",
    "cspell": "cspell --no-progress",
    "test": "npm run cspell && npm run lint"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "cspell": "5.12.3",
    "eslint": "7.32.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">= 3.7.0",
    "@typescript-eslint/parser": ">= 3.7.0",
    "eslint": ">= 7.5.0 && < 8.0.0",
    "eslint-plugin-deprecation": ">= 1.1.0",
    "eslint-plugin-simple-import-sort": ">= 6.0.1"
  }
}
