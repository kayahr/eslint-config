{
  "name": "@kayahr/eslint-config",
  "version": "3.0.0-alpha.1",
  "description": "kayahr eslint config for TypeScript projects",
  "main": "eslintrc.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayahr/eslint-config"
  },
  "funding": "https://github.com/kayahr/eslint-config?sponsor=1",
  "scripts": {
    "prepare": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc",
    "test": "run-s test:*",
    "test:lint": "eslint --max-warnings 0 --ext .ts src",
    "test:spell": "cspell --no-progress"
  },
  "devDependencies": {
    "@types/node": "20.14.11",
    "@typescript-eslint/eslint-plugin": "8.0.0-alpha.48",
    "@typescript-eslint/parser": "8.0.0-alpha.48",
    "cspell": "8.11.0",
    "eslint-plugin-deprecation": "3.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "typescript": "5.5.3"
  }
}
