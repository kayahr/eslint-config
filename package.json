{
  "name": "@kayahr/eslint-config",
  "version": "2.10.2",
  "description": "kayahr eslint config for TypeScript projects",
  "main": "eslintrc.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayahr/eslint-config"
  },
  "funding": "https://github.com/kayahr/eslint-config?sponsor=1",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "tsc",
    "prepare": "npm run compile",
    "lint": "eslint --max-warnings 0 --ext .ts src",
    "cspell": "cspell --no-progress",
    "test": "npm run cspell && npm run lint"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "cspell": "8.7.0",
    "eslint": "8.57.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "rimraf": "5.0.5",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">= 7.7.0",
    "@typescript-eslint/parser": ">= 7.7.0",
    "eslint": ">= 8.57.0",
    "eslint-plugin-deprecation": ">= 2.0.0",
    "eslint-plugin-simple-import-sort": ">= 12.1.0"
  }
}
