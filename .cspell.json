{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "version": "0.2",
    "files": [
        "**/*"
    ],
    "enabledLanguageIds": [
        "markdown",
        "typescript"
    ],
    "ignorePaths": [
        "node_modules",
        "package-lock.json",
        "git",
        "lib",
        "*.log",
        "*.tgz"
    ],
    "words": [
        "backreference",
        "backticks",
        "builtins",
        "compat",
        "eqeqeq",
        "IIFE",
        "kayahr",
        "linebreak",
        "lookaround",
        "lookarounds",
        "multilines",
        "nonblock",
        "nonconstructor",
        "nonoctal",
        "nonwords",
        "nums",
        "Reimer",
        "stroustrup",
        "TSES"
    ],
    "language": "en",
    "enableFiletypes": [
        "javascript",
        "json"
    ]
}
